{
    "manifest_version": 3,
    "name": "Smart AI Chat",
    "version": "1.0",
    "description": "Chat with AI using Smart AI.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    },
    "permissions": [
        "tabs",
        "windows"
    ]
}
